{
  "presets": [
    [
      "es2015",
      {
        "modules": false,
        "loose": true
      }
    ],
    "stage-0",
    "react"
  ],
  "plugins": [
    [
      "transform-flow-strip-types"
    ]
  ],
  "env": {
    "development": {
      "plugins": [
        "transform-react-jsx-source"
      ]
    },
    "production": {
      "plugins": [
        "lodash",
        "transform-react-remove-prop-types"
      ]
    }
  }
}
